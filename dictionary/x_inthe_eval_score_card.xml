<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Score Card" max_length="40" name="x_inthe_eval_score_card" type="collection">
        <element label="Evaluation" max_length="32" name="evaluation" reference="x_inthe_eval_evaluation" type="reference"/>
        <element label="Evaluator" max_length="32" name="evaluator" reference="sys_user" reference_qual="active=true^EQ" reference_qual_condition="active=true^EQ" type="reference"/>
        <element default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element display="true" label="Solution" max_length="32" name="solution" reference="x_inthe_eval_solution" type="reference"/>
        <element choice="3" default="in_progress" label="State" max_length="40" name="state" type="choice">
            <choice>
                <element label="In Progress" sequence="20" value="in_progress"/>
                <element label="Final" sequence="30" value="final"/>
                <element label="Complete" sequence="40" value="complete"/>
            </choice>
        </element>
        <index name="index">
            <element name="evaluation"/>
        </index>
        <index name="index2">
            <element name="solution"/>
        </index>
        <index name="index3">
            <element name="evaluator"/>
        </index>
    </element>
</database>
