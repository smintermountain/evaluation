        <index name="index">
            <element name="administrator"/>
        </index>
        <index name="index2">
            <element name="pmo_project"/>
        </index>
        <index name="index3">
            <element name="architect"/>
        </index>
        <index name="index4">
            <element name="solutions_manager"/>
        </index>
    </element>
</database>
