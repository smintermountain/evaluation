        <element choice="1" label="Criticality" mandatory="true" max_length="40" name="criticality" type="choice">
            <choice>
                <element label="Essential" sequence="10" value="essential"/>
                <element label="Preferred" sequence="20" value="preferred"/>
                <element label="Optional" sequence="30" value="optional"/>
            </choice>
        </element>
        <element label="Description" max_length="2000" name="description" type="string"/>
        <element label="Documentation" max_length="8000" name="documentation" type="html"/>
        <element label="Evaluation" max_length="32" name="evaluation" reference="x_inthe_eval_evaluation" reference_qual="stateINnew,in_progress^EQ" reference_qual_condition="stateINnew,in_progress^EQ" type="reference"/>
        <element label="Focus Area" max_length="32" name="focus_area" reference="x_inthe_eval_focus_area" reference_qual="evaluationSAMEASevaluation^EQ" reference_qual_condition="evaluationSAMEASevaluation^EQ" type="reference"/>
