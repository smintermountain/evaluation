        </element>
        <element label="Use Case" max_length="32" name="use_case" reference="x_inthe_eval_use_case" reference_qual="stateINnew,in_progress^EQ" reference_qual_condition="stateINnew,in_progress^EQ" type="reference"/>
        <index name="index">
